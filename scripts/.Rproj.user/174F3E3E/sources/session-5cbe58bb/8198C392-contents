options(rsconnect.http.timeout = 300)

rsconnect::deployApp(
  appDir = "/Users/mingruili/Documents/shiny/",  # Root directory
  appPrimaryDoc = "scripts/app_1.R",
  appFiles = c(
    "scripts/app_1.R",
